    <command name="wake">
      <pattern>wake+|get{1}\sup{1}</pattern>
      <script><![CDATA[
        if (Alarm Clock.time < 720) {
          isAlarm = false
          foreach (alarm, Alarm Clock.alarms) {
            if (alarm < Alarm Clock.time) {
              isAlarm = true
            }
          }
          if (not isAlarm) {
            msg ("You feel like there is something you need to wake up for, but you are too tired. You can... just... sleep... a little longer.")
          }
          Alarm Clock.time = Alarm Clock.time + 10
        }
        else {
          msg ("You slept in too late! It's past scheduling time and your next semester might be ruined!")
          MoveObject (player, bedroom)
          Laptop.inLaptop = false
        }
      ]]></script>
    </command>
    <command name="sleep">
      <pattern>sleep|rest|pass</pattern>
      <script><![CDATA[
        isAlarm = false
        foreach (alarm, Alarm Clock.alarms) {
          if (alarm < Alarm Clock.time) {
            isAlarm = true
          }
        }
        if (not isAlarm) {
          msg ("You continute to rest peacefully")
        }
        Alarm Clock.time = Alarm Clock.time + 55
      ]]></script>
    </command>
  </object>
  <turnscript name="TimePass">
    <enabled />
    <script><![CDATA[
      Alarm Clock.time = Alarm Clock.time + 5
      if (player.parent = Laptop.real_place) {
        Laptop.inLaptop = false
      }
      if (Alarm Clock.time % 60 = 0) {
        player.caffeine = player.caffeine - 1
      }
      if (player.caffeine = 0) {
        MoveObject (player, Sleep)
        player.caffeine = 20
      }
      if (not (Alarm Clock.snooze = -1)) {
        Alarm Clock.time = Alarm Clock.snooze
        msg ("The snoozed alarm wakes you up. You desparately need to get up. What do you do?")
        get input {
          if (IsRegexMatch("get up|move|get off", LCase(result))) {
            msg ("You successfully woke up to the alarm!")
            MoveObject (player, bedroom)
            Alarm Clock.snooze = -1
          }
          else {
            msg ("You did not get up and thus fall asleep despite the constant alarm ringing")
            Alarm Clock.snooze = -1
          }
        }
      }
      removeTime = -1
      foreach (time, Alarm Clock.alarms) {
        if (time < Alarm Clock.time) {
          removeTime = time
          if (player.parent = Sleep) {
            msg ("You hear the distant buzz of an alarm... You're... supposed.. to schedule, right? You need to get to turn it off somehow but it's on the wrong side of the bed... What do you do?")
            get input {
              if (IsRegexMatch("turn|flip|roll", LCase(result))) {
                msg ("Now you're lying with your face smothered in the pillow. It's rather uncomfortable, but you could still fall asleep like this. What do you do?")
                get input {
                  if (IsRegexMatch("turn|flip|roll", LCase(result))) {
                    msg ("Well, you're on the right side now but you can't quite remember where you set your alarm. What should you do?")
                    get input {
                      if (IsRegexMatch("look|search|find", LCase(result))) {
                        msg ("Finally! You got your alarm clock. Do you want to turn the alarm off or put it on snooze?")
                        get input {
                          if (IsRegexMatch("snooze", LCase(result))) {
                            msg ("You fall asleep again with faith that your snoozed alarm will allow you to wake up.")
                            Alarm Clock.snooze = time + 10
                          }
                          else {
                            if (IsRegexMatch("turn off|switch off", LCase(result))) {
                              msg ("You turn off the alarm, only to immediately fall asleep again. You are just too tired...")
                            }
                            else {
                              msg ("You neither turn the alarm off nor snooze it and fall back asleep.")
                            }
                          }
                        }
                      }
                      else {
                        msg ("Your alarm clock is somewhere but you cannot be bothered to find it. Oh well, might as well go back to sleep")
                      }
                    }
                  }
                  else {
                    msg ("You manage to fall asleep even though the way you are laying is uncomfortable. Despicable")
                  }
                }
              }
              else {
                msg ("You cannot quite turn around and thus fall back asleep. It's.. too hard to get up.")
              }
            }
          }
          else {
            msg ("That alarms you set just went off! Mike steps out of his room to find you. He seems angry.")
            if (Laptop.inLaptop = false) {
              MoveObject (Mike, player.parent)
              Mike.mikeTime = 5
            }
          }
        }
      }
      if (not (removeTime = -1)) {
        Alarm Clock.time = removeTime
        list remove (Alarm Clock.alarms, removeTime)
      }
    ]]></script>
  </turnscript>
  <turnscript name="MikeScript">
