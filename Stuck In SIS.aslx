    <object name="Alarm Clock">
      <inherit name="editor_object" />
      <alt type="stringlist">
        <value>clock</value>
        <value>alarm</value>
        <value>Clock</value>
        <value>Alarm</value>
      </alt>
      <take type="boolean">false</take>
      <feature_usegive />
      <attr name="feature_switchable" type="boolean">false</attr>
      <snooze type="int">-1</snooze>
      <time type="int">0</time>
      <alarms type="list" />
      <switchedon type="boolean">false</switchedon>
      <look type="script"><![CDATA[
        time = "The current time is "
        if ((9 + Alarm Clock.time/60) % 12 = 0) {
          time = time + "12:"
        }
        else {
          time = time + ((9 + Alarm Clock.time/60) % 12) + ":"
        }
        if (Alarm Clock.time % 60 < 10) {
          time = time + "0"
        }
        time = time + Alarm Clock.time % 60
        msg (time)
      ]]></look>
      <use type="script">
      </use>
      <set type="script">
        msg ("Scheduling begins tomorrow at 7:00a.m. Do you want to set an alarm?")
        get input {
          if (IsRegexMatch("yes|y", LCase(result))) {
            time = 0
            msg ("How many hours from now?")
            get input {
              if (IsRegexMatch("^[0-9]*$", result)) {
                time = time + ToInt(result) * 60
                msg ("How many minutes?")
                get input {
                  if (IsRegexMatch("^[0-9]*$", result)) {
                    time = time + ToInt(result)
                    list add (Alarm Clock.alarms, time)
                    msg ("Sucessfully added alarm")
                  }
                  else {
                    msg ("Nice try, but that is not a number!")
                  }
                }
              }
              else {
                msg ("Nice try, but that is not a number!")
              }
            }
          }
          else {
            if (IsRegexMatch("no|n", LCase(result))) {
              msg ("You choose to take the risk and not set an alarm")
            }
            else {
              msg ("Could not parse input. Say yes or no next time")
            }
          }
        }
      </set>
    </object>
