 <object name="Desktop">
    <inherit name="editor_room" />
    <isroom />
    <description>You have several applications open. You definitely opened all of them for a reason, but have forgotten exactly why.</description>
    <beforeenter type="script">
      create exit ("Leave", Desktop, Laptop.real_place)
    </beforeenter>
    <onexit type="script">
      MakeExitInvisible (GetObject(GetExitByLink(Desktop, Laptop.real_place )))
    </onexit>
    <enter type="script">
    </enter>
